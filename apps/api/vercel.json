{
  "functions": {
    "api/index.ts": {
      "runtime": "@vercel/node@3.2.0",
      "includeFiles": "prisma/**"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.ts"
    },
    {
      "source": "/health",
      "destination": "/api/index.ts"
    }
  ]
}
